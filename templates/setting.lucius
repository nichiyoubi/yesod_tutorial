h1 {
color:#000000;
}

h2 {
color:#303030;
}

#title {
       color:#303030;
}
.title {
       font-size: x-large;
}

#month {
	display: table;
}
.prev{
	display: table-cell;
	width: 50px;
}
.year{
	display: table-cell;
	width: 110px;
	font-size: large;
}
.next{
	display: table-cell;
	width: 50px;
}

#calendar {
	display: table;
}
.wday{
	display: table-row;
}
.wday>div {
	display: table-cell;
	width: 30px;
}
.week {
	display: table-row;
}
.week>div {
	display: table-cell;
	width: 30px;
}

